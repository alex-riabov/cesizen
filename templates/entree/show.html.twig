{% extends 'base.html.twig' %}

{% block title %}Entree{% endblock %}

{% block body %}
    <h1>Entree</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entree.id }}</td>
            </tr>
            <tr>
                <th>DateHeureEntree</th>
                <td>{{ entree.dateHeureEntree ? entree.dateHeureEntree|date('Y-m-d H:i:s') : '' }}</td>
